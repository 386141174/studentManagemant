<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.inventec.studentManagement.Dao.TestDao">
	<insert id="test" parameterType="Test">
		insert into dbo.STUDENTS(student_sname,student_ssex,student_saddress) values(#{student_sname},#{student_ssex},#{student_saddress})
	</insert>
	

	<select id="select" resultType="int">
		select count(1) from dbo.STUDENTS
	</select>


</mapper>